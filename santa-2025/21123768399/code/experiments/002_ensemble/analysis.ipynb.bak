    "# Tree geometry - exact from competition\n",
    "def get_tree_vertices():\n",
    "    trunk_w = 0.15\n",
    "    trunk_h = 0.2\n",
    "    base_w = 0.7\n",
    "    mid_w = 0.4\n",
    "    top_w = 0.25\n",
    "    tip_y = 0.8\n",
    "    tier_1_y = 0.5\n",
    "    tier_2_y = 0.25\n",
    "    base_y = 0.0\n",
    "    trunk_bottom_y = -trunk_h\n",
    "    \n",
    "    vertices = [\n",
    "        (0.0, tip_y),\n",
    "        (top_w / 2, tier_1_y),\n",
    "        (top_w / 4, tier_1_y),\n",
    "        (mid_w / 2, tier_2_y),\n",
    "        (mid_w / 4, tier_2_y),\n",
    "        (base_w / 2, base_y),\n",
    "        (trunk_w / 2, base_y),\n",
    "        (trunk_w / 2, trunk_bottom_y),\n",
    "        (-trunk_w / 2, trunk_bottom_y),\n",
    "        (-trunk_w / 2, base_y),\n",
    "        (-base_w / 2, base_y),\n",
    "        (-mid_w / 4, tier_2_y),\n",
    "        (-mid_w / 2, tier_2_y),\n",
    "        (-top_w / 4, tier_1_y),\n",
    "        (-top_w / 2, tier_1_y),\n",
    "    ]\n",
    "    return vertices\n",
    "\n",
    "BASE_TREE = Polygon(get_tree_vertices())\n",
    "print(f\"Base tree bounds: {BASE_TREE.bounds}\")"
   ]
  },
